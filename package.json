{
  "name": "github-pages",
  "packageManager": "yarn@4.9.4",
  "dependencies": {
    "pug": "^3.0.3",
    "serve-handler": "^6.1.6",
    "ts-patch": "^3.3.0",
    "typescript": "^5.9.3",
    "typescript-transform-paths": "^3.5.6"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^10.0.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/eslint": "^9.6.1",
    "@types/katex": "^0.16.8",
    "@types/node": "^25.3.0",
    "@types/p5": "^1.7.7",
    "@types/pug": "^2.0.10",
    "@types/serve-handler": "^6.1.4",
    "@types/three": "^0.183.1",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^10.0.0",
    "katex": "^0.16.28",
    "mathjs": "^15.1.1",
    "p5": "^2.2.2",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.3",
    "three": "^0.183.1"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:bin": "tsc -p tsconfig.build.json",
    "dev": "node dist/bin/build.js --dev --watch --serve",
    "build": "node dist/bin/build.js",
    "serve": "node dist/bin/build.js --no-build --serve",
    "watch": "node dist/bin/build.js --watch --serve",
    "deps": "node dist/bin/dependency-report.js",
    "typecheck": "tsc --noEmit && tsc -p tsconfig.build.json --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --ignore-path .gitignore --write .",
    "format:check": "prettier --ignore-path .gitignore --check .",
    "validate": "yarn run typecheck && yarn run lint && yarn run format:check"
  }
}
